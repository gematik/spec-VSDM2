{
  "resourceType": "StructureDefinition",
  "id": "VSDMBeihilfe",
  "url": "https://gematik.de/fhir/vsdm2/StructureDefinition/VSDMBeihilfe",
  "version": "1.0.0",
  "name": "VSDMBeihilfe",
  "title": "Beihilfeanspruch",
  "status": "draft",
  "experimental": false,
  "date": "2023-12-31",
  "publisher": "gematik GmbH",
  "description": "Angaben zum Beihilfe-Anspruch eines PKV-Versicherten im Versichertenstammdatenmanagement (VSDM) 2.0",
  "fhirVersion": "4.0.1",
  "kind": "complex-type",
  "abstract": false,
  "context": [
    {
      "expression": "Coverage",
      "type": "element"
    }
  ],
  "type": "Extension",
  "baseDefinition": "http://hl7.org/fhir/StructureDefinition/Extension",
  "derivation": "constraint",
  "differential": {
    "element": [
      {
        "id": "Extension",
        "path": "Extension",
        "short": "Beihilfeanspruch",
        "definition": "Angaben zum Beihilfe-Anspruch eines PKV-Versicherten im Versichertenstammdatenmanagement (VSDM) 2.0.\nDiese Struktur ist für Versicherte ohne Beihilfeanspruch nicht anzugeben.      ",
        "comment": "Hintergrundinformationen siehe https://www.pkv.de/wissen/private-krankenversicherung/leistungen-und-erstattung/privatversichert-im-beamtenverhaeltnis/"
      },
      {
        "id": "Extension.extension",
        "path": "Extension.extension",
        "min": 1
      },
      {
        "id": "Extension.extension:anspruch",
        "path": "Extension.extension",
        "sliceName": "anspruch",
        "short": "Versicherter mit Beihilfeanspruch",
        "definition": "Kennzeichnet einen Versicherten mit Beihilfeanspruch.\nDieses Kennzeichen ist anzugeben, sobald die Struktur Beihilfe erforderlich wird. \nDas ist notwendig, da die Beihilfequote nicht immer vorliegt. ",
        "min": 1,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Extension.extension:anspruch.extension",
        "path": "Extension.extension.extension",
        "max": "0"
      },
      {
        "id": "Extension.extension:anspruch.url",
        "path": "Extension.extension.url",
        "fixedUri": "anspruch"
      },
      {
        "id": "Extension.extension:anspruch.value[x]",
        "path": "Extension.extension.value[x]",
        "min": 1,
        "type": [
          {
            "code": "boolean"
          }
        ]
      },
      {
        "id": "Extension.extension:quote",
        "path": "Extension.extension",
        "sliceName": "quote",
        "short": "Quote PKV bei Beihilfe",
        "definition": "Prozentwert des über die Beihilfe hinaus in der PKV versicherten Anteil.",
        "comment": "Prozentwert (0..100)",
        "min": 0,
        "max": "1",
        "mustSupport": true
      },
      {
        "id": "Extension.extension:quote.extension",
        "path": "Extension.extension.extension",
        "max": "0"
      },
      {
        "id": "Extension.extension:quote.url",
        "path": "Extension.extension.url",
        "fixedUri": "quote"
      },
      {
        "id": "Extension.extension:quote.value[x]",
        "path": "Extension.extension.value[x]",
        "type": [
          {
            "code": "unsignedInt"
          }
        ],
        "constraint": [
          {
            "key": "unsignedInt-percentage-1",
            "severity": "error",
            "human": "Prozentwerte müssen im Bereich von 0 bis 100 liegen.",
            "expression": "($this >= 0) and ($this <= 100)",
            "source": "https://gematik.de/fhir/vsdm2/StructureDefinition/VSDMBeihilfe"
          }
        ]
      },
      {
        "id": "Extension.url",
        "path": "Extension.url",
        "fixedUri": "https://gematik.de/fhir/vsdm2/StructureDefinition/VSDMBeihilfe"
      },
      {
        "id": "Extension.value[x]",
        "path": "Extension.value[x]",
        "max": "0"
      }
    ]
  }
}
